@using Microsoft.AspNetCore.Identity;
@inject SignInManager<Users> SignInManager;

	<ul class="navbar-nav ms-auto">
	@if (SignInManager.IsSignedIn(User))
	{
		<li class="nav-item">
			<a class="nav-link  text-white" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
		</li>
		<li class="nav-item">
			<a class="nav-link  text-white" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
		</li>
		<li class="nav-item">
			<a class="nav-link  text-white" asp-area="" asp-controller="Home" asp-action="Announcement">Announcement</a>
		</li>
		<li class="nav-item">
			<a class="nav-link  text-white" asp-area="" asp-controller="Home" asp-action="Ameneties">Ameneties</a>
		</li>
		<li class="nav-item end-0">
			<a class="nav-link  text-white" asp-controller="Account" asp-action="Logout">Logout</a>
		</li>
	}
	else
	{
		<li class="nav-item end-0">
			<a class="nav-link nav-link  text-white" asp-controller="Account" asp-action="Login">Login</a>
		</li>
		<li class="nav-item end-0">
			<a class="nav-link nav-link  text-white" asp-controller="Account" asp-action="Register">Register</a>
		</li>
	}
	</ul>
